<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Frame name="M0L_Frame" toplevel="true" movable="true" parent="UIParent" inherits="OptionFrameBoxTemplate" enableMouse="true" registerForClicks="AnyUp">
		<Size>
			<AbsDimension x="120" y="95"/>
		</Size>
		<Anchors>
			<Anchor point="TOPRIGHT" relativeTo="PlayerFrame" relativePoint="TOPRIGHT">
				<Offset>
					<AbsDimension x="120" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">

				<FontString name = "M0L_NameString" inherits="GameFontDisable" text="MobsOnLevel">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="34"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- Time remaining -->
				<FontString inherits="GameFontNormalSmall" text="Time remaining:">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="16"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!--  Time string -->
				<FontString name = "M0L_TimeString" inherits="GameFontWhite" text="0">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="4"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- Mobs remaining -->
				<FontString name = "M0L_MobString" inherits="GameFontDisableSmall" text="Mobs remaining:">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="-10"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- MobsOnLevel -->
				<FontString name = "M0L_String" inherits="GameFontWhite" text="0">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="-24"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- Hidden -->
				<FontString name = "M0L_HiddenString" inherits="GameFontDisableSmall" text="(hidden)">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-32" y="-36"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- Version -->
				<FontString name = "M0L_VerString" inherits="GameFontDisableSmall" text="v0">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="32" y="-36"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				this:SetBackdropColor(0, 0, 0, .2);
				this:RegisterForDrag("LeftButton");
				M0L_OnLoad();
			</OnLoad>
			<OnDragStart>
				this:StartMoving();
			</OnDragStart>
			<OnDragStop>
				this:StopMovingOrSizing();
			</OnDragStop>
			<OnMouseUp>
				this:StopMovingOrSizing();
				M0L_OnClick();
			</OnMouseUp>
			<OnEvent>
				M0L_OnEvent();
			</OnEvent>
		</Scripts>
	</Frame>
</Ui> 
