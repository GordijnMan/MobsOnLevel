<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Frame name="M0L_Frame" toplevel="true" movable="true" parent="UIParent" inherits="OptionFrameBoxTemplate">
		<Size>
			<AbsDimension x="110" y="80"/>
		</Size>
		<Anchors>
			<Anchor point="TOPRIGHT" relativeTo="PlayerFrame">
				<Offset>
					<AbsDimension x="80" y="-100"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">

				<!-- Time to level -->
				<FontString inherits="GameFontNormalSmall" text="Time remaining:">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="24"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name = "M0L_TimeString" inherits="GameFontWhite" text="0">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="10"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>

				<!-- Kills to level -->
				<FontString inherits="GameFontNormalSmall" text="Mobs remaining:">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="-8"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name = "M0L_String" inherits="GameFontWhite" text="0">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="-22"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				this:SetBackdropColor(0.1, 0.1, 0.1, 0.2);
				this:RegisterForDrag("LeftButton");
				M0L_OnLoad();
			</OnLoad>
			<OnDragStart>
				this:StartMoving();
			</OnDragStart>
			<OnDragStop>
				this:StopMovingOrSizing();
			</OnDragStop>
			<OnMouseUp>
				this:StopMovingOrSizing();
			</OnMouseUp>
			<OnEvent>
				M0L_OnEvent();
			</OnEvent>
		</Scripts>
	</Frame>
</Ui> 
